{
    "name": "imemento/sdk",
    "description": "SDK classes for iMemento Services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.5",
        "illuminate/contracts": "^5.5",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "IMemento\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMemento\\SDK\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IMemento\\SDK\\ServiceProvider"
            ]
        }
    }
}
