{
    "name": "imemento/clients",
    "description": "Client library classes for iMemento Services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "opis/closure": "^3.1",
        "illuminate/support": "^8.0",
        "illuminate/contracts": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "imemento/sdk-auth": "~8.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Clients\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Clients\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "iMemento\\Clients\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
